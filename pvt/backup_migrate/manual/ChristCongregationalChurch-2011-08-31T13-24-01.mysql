INSERT INTO `field_revision_body` VALUES ('node','page','0','81','116','und','0','<p><img alt="" src="/d/sites/default/files/pictures/hello-my-name-is.jpg" style="float: right; width: 300px; height: 203px; ">We know that it can be intimidating to visit a new church. &nbsp;Our hope is that you will feel right at home when you come to Christ Congregational Church.</p><p><strong>Good People</strong></p><p>First of all, know that no perfect people are at our church. &nbsp;We are ordinary people who are sharing life and discovering the joy of friendship and faith. &nbsp;So, come as you are - you''ll fit right in.</p><p>&nbsp;</p><p><strong>Our Location</strong></p><p><a href="http://visitchristchurch.org/about/location" style="border-style: initial; border-color: initial; font-style: inherit; font-size: 12px; font-family: inherit; color: rgb(22, 62, 85); text-decoration: underline; " target="_blank"><img alt="icon" src="http://visitchristchurch.org/d/sites/all/themes/bluemasters/images/location.jpg" style="margin-right: 8px; margin-left: 8px; border-style: initial; border-color: initial; font-style: inherit; font-size: 12px; font-family: inherit; border-style: initial; border-color: initial; border-width: initial; border-color: initial; border-width: initial; border-color: initial; float: left; width: 32px; height: 32px; ">2500 South Sheridan Blvd<br>Denver, CO 80227</a></p><p><strong>Our Service Times</strong></p><p>&nbsp;</p><p><b>Need a ride or a guide?</b></p><p><a href="/contact">Let us know you are coming</a>, and we can have a host ready to meet you and find your place. &nbsp;Our host will introduce you to our pastor and other people who will make you feel right at home. &nbsp;If you have special needs or need a ride, please let us know. &nbsp;</p>','','filtered_html'),('node','event','0','83','118','und','0','<p>Children''s church and nursery are availble for our Sunday morning services.</p>','','filtered_html'),('node','event','0','84','119','und','0','<p>ths;alkjsd;lkjs;lkdsj;lskjdsf;lkjfsd</p><p>&nbsp;</p><p>sd;klds;lk</p>','','filtered_html');
