INSERT INTO `block_custom` VALUES ('1','<p>All About Our Church</p><div class="content"><p>Lorem ipsum dolor sit amet, consect- etur adipiscing elit. Praesent tempor facilisis congue. Lorem sum dolor sit amet, consectetur adippend...</p><ul class="links inline"><li class="comment-add first"><a href="node/3" title="Add a new comment to this page.">Learn more</a></li></ul></div><p>&nbsp;</p>','About','full_html'),('2','<p>sermons</p><p>photos</p>','Media','filtered_html'),('3','<p><a href="tel:303 935-8350"><img alt="icon" src="/d/sites/all/themes/bluemasters/images/phone2.png" style="margin-left: 8px; margin-right: 8px; ">(303) 935-8350</a></p><div><p><img alt="icon" src="/d/sites/all/themes/bluemasters/images/mail.png" style="margin-left: 8px; margin-right: 8px; "><a href="mailto:rondel.axort@gmail.com">church@email.com</a></p><p><a href="/about/location" target="_blank"><img alt="icon" src="/d/sites/all/themes/bluemasters/images/location.jpg" style="margin-left: 8px; margin-right: 8px; float: left; width: 32px; height: 32px; ">2500 South Sheridan Blvd<br>Denver, CO 80227</a></p></div><p>&nbsp;</p>','Contact','full_html'),('4','<p style="text-align: center; "><a href="/new"><img alt="" src="/d/sites/default/files/pictures/hello-my-name-is.jpg" style="width: 120px; height: 81px; "></a></p><p style="text-align: center; "><a href="/new">Click here to learn about our church<br>and how to get involved.</a></p>','New','full_html');
