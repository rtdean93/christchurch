DROP TABLE IF EXISTS `aggregator_item`;
CREATE TABLE `aggregator_item` (   `iid` int(11) NOT NULL auto_increment COMMENT 'Primary Key: Unique ID for feed item.',   `fid` int(11) NOT NULL default '0' COMMENT 'The aggregator_feed.fid to which this item belongs.',   `title` varchar(255) NOT NULL default '' COMMENT 'Title of the feed item.',   `link` varchar(255) NOT NULL default '' COMMENT 'Link to the feed item.',   `author` varchar(255) NOT NULL default '' COMMENT 'Author of the feed item.',   `description` longtext NOT NULL COMMENT 'Body of the feed item.',   `timestamp` int(11) default NULL COMMENT 'Posted date of the feed item, as a Unix timestamp.',   `guid` varchar(255) default NULL COMMENT 'Unique identifier for the feed item.',   PRIMARY KEY  (`iid`),   KEY `fid` (`fid`) ) ENGINE=InnoDB AUTO_INCREMENT=17 DEFAULT CHARSET=utf8 COMMENT='Stores the individual items imported from feeds.' AUTO_INCREMENT=17;
INSERT INTO `aggregator_item` VALUES ('11','1','Mass. Conference Minister arrested at White House pipeline protest','http://feedproxy.google.com/~r/UnitedChurchOfChrist/~3/w8fZ8EdztB8/mass-conference-minister.html','','The Rev. Jim Antal, the UCC''s Massachusetts Conference Minister, was one of 65 people from across the US and Canada arrested at the White House Aug. 20, 2011, the first day of a two week sit-in aimed at pressuring President Obama to deny the permit for a massive new oil pipeline.','1313989200','www.ucc.org-264363284'),('12','1','Small Ohio UCC church dreams big for Mission:1','http://feedproxy.google.com/~r/UnitedChurchOfChrist/~3/P9WdVoKHiYE/still-11-weeks-away-from-the.html','','Still 11 weeks away from the kickoff of Mission:1, little Bethany UCC has sky-high hopes for the national church’s campaign against hunger.','1313989200','www.ucc.org-264314869'),('13','1','Calling Congregations conference invites ‘young, transformative’ leaders','http://feedproxy.google.com/~r/UnitedChurchOfChrist/~3/xTZckwRVsak/calling-congregations-1.html','','Hosted by the Fund for Theological Education, the 2011 Calling Congregations Conference will be held Oct. 6-8 at the Georgia Tech Hotel & Conference Center in Atlanta.','1313989200','www.ucc.org-264320678'),('14','1','Mass. Conference Minister cites irony at timing of protester arrests, planned King ceremony','http://feedproxy.google.com/~r/UnitedChurchOfChrist/~3/nNVuw_wXDY4/mass-conference-minister-1.html','','For the Rev. Jim Antal, the irony loomed like the big white blob depicting Hurricane Irene on the national weather map.','1314334800','www.ucc.org-265727077'),('15','1','UCC’s ''Mission:1'' goals expanded to include $111,111 church-wide appeal for East Africa famine relief','http://feedproxy.google.com/~r/UnitedChurchOfChrist/~3/cyZVwtHKOS0/uccs-mission1-goals.html','','Mission:1, the United Church of Christ''s nationwide 11-day campaign in early November to feed the hungry and confront food-related injustice, is being expanded to include a church-wide appeal to raise more than $111,111 to aid famine-stricken Somalia, Kenya and Ethiopia.','1314594000','www.ucc.org-266442068');
INSERT INTO `aggregator_item` VALUES ('16','1','Hurricane floodwaters pack punch to Vermont UCC church members','http://feedproxy.google.com/~r/UnitedChurchOfChrist/~3/VVZG_8V4dKk/hurricane-floodwaters-pack.html','','Responsible for 38 deaths and days-long power outages to more than 3.3 million people, Hurricane Irene has left a mucky mess for many UCC church members in the Vermont Conference.','1314680400','www.ucc.org-266772619');
DROP TABLE IF EXISTS `authmap`;
